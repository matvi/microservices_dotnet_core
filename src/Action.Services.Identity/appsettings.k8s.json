          "AutoDelete": true,
          "Durable": true,
          "Exclusive": true
      }  
  },
  "jwt" : {
    "secretKey" : "supperSecretPassword",
    "expireMinutes" : 5,
    "issuer" : "https://localhost:5001"
  },
