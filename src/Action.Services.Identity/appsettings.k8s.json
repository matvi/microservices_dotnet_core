{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "RabbitMq" : {   
      "Username": "guest",
      "Password": "guest",
      "VirtualHost": "/",
      "Port": 5672,
      "Hostnames": [ "rabbitmq-0.rabbitmq" ],
